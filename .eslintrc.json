{
	"env": {
		"browser": true,
		"es2021": true,
		"es6": true,
		"node": true
	},

	"extends": ["prettier"],
	"plugins": ["@typescript-eslint", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "error",
		"func-names": "error"
	}
}
